{
  "README": [
    "Do not edit this file (defaults.json). It can break the things.",
    "Instead, copy the following values to your JSDoc config, then edit them as you please."
  ],
  "templates": {
    "docolatte": {
      "import": [],
      "minify": true,
      "data": {
        "exclude": [],
        "removeOrphans": true
      },
      "toc": {
        "icons": {
          "variables": { "hide": false },
          "functions": { "hide": false }
        },
        "menus": {
          "tutorials":  "Tutorials",
          "modules":    "Modules",
          "externals":  "Externals",
          "namespaces": "Namespaces",
          "classes":    "Classes",
          "interfaces": "Interfaces",
          "events":     "Events",
          "mixins":     "Mixins",
          "globals":    "Global"
        }
      },
      "lightSwitch": {
        "hide": false,
        "default": "auto",
        "icons": {
          "auto":  "halfmoon",
          "light": "sun",
          "dark":  "moon"
        }
      },
      "meta": {
        "lang": "en",
        "title": null,
        "description": null,
        "keywords": null,
        "author": null,
        "favicon": []
      },
      "branding": {
        "title": "My Project",
        "link": "index.html",
        "icon": "home",
        "font": {
          "size": null,
          "family": null
        }
      },
      "home": {
        "package": {
          "hide": false
        }
      },
      "readme": {
        "truncate": true,
        "emoji": {
          "replace": "colons",
          "options": {
            "replace_mode": "unified",
            "allow_native": true
          }
        }
      },
      "footer": {
        "hide": false,
        "copyright": null,
        "license": null,
        "hideGenerator": false
      },
      "code": {
        "theme": "base16/espresso",
        "examples": {
          "captionPrefix": [
            "// ",
            "- ",
            "# "
          ]
        }
      },
      "search": {
        "limit": 8,
        "placeholder": "type to searchâ€¦",
        "hint": {
          "hide": false,
          "body": [
            "Type <em>any key</em> to focus."
          ]
        },
        "keys": [
          { "name": "name",           "weight": 10 },
          { "name": "longname",       "weight": 9 },
          { "name": "meta.shortpath", "weight": 6 },
          { "name": "classdesc",      "weight": 2 },
          { "name": "description",    "weight": 2 }
        ]
      }
    }
  }
}
