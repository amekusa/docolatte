{
  "name": "docolatte",
  "version": "1.0.0",
  "description": "cool JSDoc 3 template",
  "main": "publish.js",
  "scripts": {
    "build": "lessc --source-map static/styles/less/theme.less static/styles/theme.css",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": "static/styles/less",
      "extensions": "less"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amekusa/docolatte.git"
  },
  "author": "amekusa (https://amekusa.com)",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/amekusa/docolatte/issues"
  },
  "homepage": "https://github.com/amekusa/docolatte",
  "devDependencies": {
    "less": "^3.12.2"
  }
}
