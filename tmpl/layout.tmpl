<?js
var { theme } = this;
var $      = theme.util;
var config = theme.config;
?>
<!DOCTYPE html>
<?js= $.elem({
    name: 'html',
    close: false,
    attrs: {
        'lang': config.meta.lang,
        'data-color-scheme': config.lightSwitch.default
    }
})?>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title><?js= $.e($.if(title != 'Home', `${title} | `) + config.meta.title)?></title>
    <?js= $.iterate([
        'description',
        'keywords',
        'author'
    ], item => {
        if (config.meta[item]) {
            return `<meta name="${item}" content="${$.e(config.meta[item])}">`;
        }
    })?>
    <?js= $.iterate(config.meta.favicon, item =>
        `<link rel="icon" href="${item}">`,
        `<link rel="icon" title="dummy" href="data:image/x-icon;base64,AA">`
    )?>
    <?js= $.iterate(theme.files.script, item =>
        `<script src="${item.url}"></script>`
    )?>
    <?js= $.iterate(theme.files.style, item =>
        `<link type="text/css" rel="stylesheet" href="${item.url}">`
    )?>
    <noscript>
        <style>
            [data-ready] {
                visibility: visible !important;
            }
        </style>
    </noscript>
    <?js if (config.style) { ?>
    <style><?js= config.style ?></style>
    <?js } ?>
</head>
<body>
<div class="page">

    <header class="header">
        <div class="masthead">
            <?js if (config.branding.link) { ?>
            <a class="link" href="<?js= config.branding.link ?>">
                <?js= $.icon(config.branding.icon || 'home') ?>
            </a>
            <?js } ?>
            <a class="title" href="index.html"><?js= config.branding.title ?></a>
        </div>
        <div class="topbar">
            <h1 class="page-title">
                <a href="#"><?js= title ?></a>
            </h1>
            <?js if (!config.lightSwitch.hide) { ?>
            <a title="Switch light/dark mode" href="#" class="light-switch" data-state="<?js= config.lightSwitch.default ?>">
                <span class="light"><?js= $.icon(config.lightSwitch.icons.light) ?></span>
                <span class="dark"><?js= $.icon(config.lightSwitch.icons.dark) ?></span>
            </a>
            <?js } ?>
        </div>
    </header>

    <div class="primary<?js= config.footer.hide ? '' : ' has-footer' ?>">
        <main class="main">
            <?js= content ?>
        </main>
        <footer class="footer">
            <?js if (!config.footer.hide) { ?>
            <div class="lines">
                <?js if (config.footer.copyright) { ?>
                <p class="copyright"><?js= config.footer.copyright ?></p>
                <?js } ?>
                <?js if (config.footer.license) { ?>
                <p class="license"><?js= config.footer.license ?></p>
                <?js } ?>
                <?js if (!config.footer.hideGenerator) { ?>
                <p class="generator">
                    Documentation generated by <a rel="external" href="https://github.com/jsdoc/jsdoc">JSDoc <?js= env.version.number ?></a><wbr>
                    using <a rel="external" href="https://github.com/amekusa/docolatte">Docolatte</a> theme
                    <?js if (env.conf.templates && env.conf.templates.default && env.conf.templates.default.includeDate !== false) { var date = new Date(); ?>
                    <wbr>on <time datetime="<?js= date.toISOString() ?>"><?js= date.toGMTString() ?></time>
                    <?js } ?>
                </p>
                <?js } ?>
            </div>
            <?js } ?>
            <div class="bottom">
                <a class="to-top" href="#">
                    <div class="label">Top<?js= $.icon('chevron-up') ?></div>
                </a>
            </div>
        </footer>
    </div>

    <input id="docolatte-sidebar-toggle" type="checkbox">
    <label class="overlay" for="docolatte-sidebar-toggle"></label>
    <label class="menu-button" for="docolatte-sidebar-toggle">
        <div class="shape">
            <i class="icon burger">
                <span class="top"></span>
                <span class="middle"></span>
                <span class="bottom"></span>
            </i>
        </div>
    </label>

    <div class="sidebar">
        <div class="toc" data-ready="0">
            <?js= this.nav ?>
        </div>
    </div>
</div>

<!-- %DOCOLATTE::DATA_GOES_HERE% -->
<script>
    var _SEARCH = {
        list: document.getElementById('docolatte-search-list').innerHTML,
        index: document.getElementById('docolatte-search-index').innerHTML,
        options: document.getElementById('docolatte-search-options').innerHTML
    };
</script>

</body>
</html>
